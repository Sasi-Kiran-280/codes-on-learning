#include<stdio.h> 
int main(){

    int n1,n2,temp,num,sum,fact=1,a,rem;
   printf("enter first limit");
   scanf("%d",&n1);
   printf("enter last limit");
   scanf("%d",&n2);
   
    for(num=n1;num<=n2;num++)
    {
        temp=num;
        sum=0;
        while(temp>0){
            fact=1;
          rem=temp%10;
        
        for(a=1;a<=rem;a++){
            fact=fact*a;
            }
            sum=sum+fact;
            temp=temp/10;
        }
        if(sum==num){
            printf("%d,",num);
        }
    }
    
    return 0;
}